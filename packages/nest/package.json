{
  "name": "@swiftpost/nest",
  "version": "0.0.2",
  "author": "Jon Manga",
  "description": "A nestjs module for @swiftpost/mail. Allows you to send emails with swiftpost.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/celleb/swiftpost.git",
    "directory": "packages/nest"
  },
  "bugs": {
    "url": "https://github.com/celleb/swiftpost/issues"
  },
  "homepage": "https://github.com/celleb/swiftpost/tree/main/packages/nest#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "prepack": "pnpm build",
    "prepublish": "pnpm build",
    "test": "vitest run"
  },
  "keywords": [
    "email",
    "authentication",
    "templ",
    "swiftpost",
    "nestjs"
  ],
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "10.4.7",
    "@swiftpost/mail": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@nestjs/testing": "catalog:"
  },
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "onFail": "error"
    }
  }
}
